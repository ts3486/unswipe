{
  "name": "unmatch",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "preflight": "pnpm run typecheck && pnpm run lint && pnpm run test",
    "clean:metro": "npx expo start --clear --no-dev --minify=false & sleep 5 && kill $!; true",
    "clean:ios": "rm -rf ios/build ios/DerivedData ios/Pods && cd ios && pod install",
    "clean:all": "rm -rf node_modules/.cache .expo && watchman watch-del-all 2>/dev/null; pnpm install && cd ios && pod install",
    "e2e": "maestro test e2e/flows",
    "e2e:onboarding": "maestro test e2e/flows/01-onboarding.yaml",
    "e2e:panic": "maestro test e2e/flows/02-panic-swipe.yaml e2e/flows/03-panic-spend.yaml",
    "e2e:settings": "maestro test e2e/flows/04-settings.yaml",
    "e2e:data": "maestro test e2e/flows/05-export-delete.yaml",
    "e2e:studio": "maestro studio",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "ship:ios": "eas build --platform ios --profile production --auto-submit",
    "ship:android": "eas build --platform android --profile production --auto-submit",
    "ship:all": "eas build --platform all --profile production --auto-submit"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/native": "^7.1.8",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "expo": "~54.0.33",
    "expo-constants": "~18.0.13",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "^6.0.20",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.23",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-sqlite": "~16.0.5",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-paper": "^5.14.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.11.0",
    "react-native-view-shot": "4.0.3",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
